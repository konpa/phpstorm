<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Add to VCS">
        <Item>
          <IdeaActionID>ChangesView.AddUnversioned.From.Dialog</IdeaActionID>
          <ShortCut>'Ctrl+Alt+A'</ShortCut>
          <Description>Add to VCS</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Commit...">
        <Item>
          <IdeaActionID>CheckinProject</IdeaActionID>
          <ShortCut>'Ctrl+K'</ShortCut>
          <Description>Commit...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Jump to Navigation Bar">
        <Item>
          <IdeaActionID>ShowNavBar</IdeaActionID>
          <ShortCut>'Alt+Origine'</ShortCut>
          <Description>Jump to Navigation Bar</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>20</Count>
        </Item>
      </Statistic>
      <Statistic Action="Version Control">
        <Item>
          <IdeaActionID>ActivateVersionControlToolWindow</IdeaActionID>
          <ShortCut>'Alt+9' or 'Alt+Maj+9'</ShortCut>
          <Description>Version Control</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>